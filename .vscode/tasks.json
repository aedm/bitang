{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"--profile",
				"dev-nowarn"
			],
			"group": "test",
			"label": "default-run",
			"options": {
				"env": {
					"RUSTFLAGS": "-A dead_code -A unused_variables -A unused_mut -A unused_imports",
					"RUST_BACKTRACE": "1",
					"RUST_LOG": "debug"
				}
			}
		}
	]
}
