{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"args": ["--profile", "dev-nowarn"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo build",
			"options": {
				"env": {
					"RUSTFLAGS": "-A dead_code -A unused_variables -A unused_mut -A unused_imports"
				}
			}
		}
	]
}